package project.transito.models;

import java.util.List;

import javax.persistence.*;
import javax.validation.constraints.NotEmpty;

import org.springframework.validation.annotation.Validated;

import lombok.Data;

@Entity
@Table (name="via")
@Validated
@Data 
@NamedQueries({
	@NamedQuery(name=Via.class, query="")
})
public class Via {
	
	public static final String  LIST_VIA_APTAS ="LIST_VIA_APTAS";
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private int identificador;
	
	@NotEmpty
	private String tipo;
	@NotEmpty
	private String CalleCarrera;
	
	private double nivelCongestion;
	
	private int numero;
	
	@OneToMany(mappedBy = "via")
	private List<AgenteTransito>listAgente;

}
